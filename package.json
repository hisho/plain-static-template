    "webpack-fix-style-only-entries": "^0.6.1",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "@hisho/reset.css": "^1.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "tailwindcss": "^2.2.4"
  }
}
