{
  "name": "plain-static-template",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=14.8.0"
  },
  "scripts": {
    "preinstall": "npx check-engines && npx only-allow npm",
    "postinstall": "NODE_ENV=production MODE=development run-s gulp-mkdir",
    "prestart": "run-s _pre",
    "start": "NODE_ENV=development run-s _start",
    "start-pro": "NODE_ENV=production MODE=start run-s _start",
    "prebuild": "run-s _pre",
    "build": "NODE_ENV=production MODE=build run-s _build",
    "build-dev": "NODE_ENV=development MODE=build run-s _build",
    "postbuild": "run-s _post",
    "serve": "NODE_ENV=production MODE=build run-s build server",
    "server": "browser-sync start --config ./.config/bs-config.js",
    "gulp-clean": "gulp clean",
    "gulp-clone": "gulp clone",
    "gulp-mkdir": "gulp mkdir",
    "gulp-image": "gulp image",
    "gulp-generateImageConstant": "gulp generateImageConstant",
    "gulp-html": "gulp html",
    "gulp-watch": "gulp watch",
    "gulp-sprite": "gulp sprite",
    "gulp-build": "run-p gulp-clone gulp-image gulp-generateImageConstant gulp-sprite",
    "webpack-watch": "webpack -w",
    "webpack-build": "webpack",
    "watch:gulp-webpack": "run-p gulp-watch webpack-watch",
    "build:gulp-webpack": "run-p gulp-build webpack-build",
    "_pre": "run-s gulp-clean",
    "_post": "run-s gulp-html",
    "_start": "run-p server gulp-build watch:gulp-webpack",
    "_build": "run-s build:gulp-webpack"
  },
  "devDependencies": {
    "@types/browser-sync": "^3001.0.28-alpha",
    "@types/glob": "^7.1.3",
    "@types/node": "^15.3.0",
    "@types/react": "^17.0.5",
    "@types/react-dom": "^17.0.5",
    "@types/react-helmet": "^6.1.1",
    "@types/webpack": "^5.28.0",
    "autoprefixer": "^9.8.6",
    "browser-sync": "^2.26.14",
    "chalk": "^2.4.2",
    "css-loader": "^5.2.4",
    "dayjs": "^1.10.4",
    "del": "^6.0.0",
    "extract-css-chunks-webpack-plugin": "^4.9.0",
    "fork-ts-checker-webpack-plugin": "^6.2.9",
    "fs-extra": "^9.0.1",
    "glob": "^7.1.7",
    "gulp": "^4.0.2",
    "gulp-htmlhint": "^4.0.1",
    "gulp-imagemin": "^7.1.0",
    "gulp-notify": "^4.0.0",
    "gulp-pretty-html": "^2.0.10",
    "gulp-rename": "^2.0.0",
    "gulp-svg-sprite": "^1.5.0",
    "gulp-webp": "^4.0.1",
    "html-webpack-plugin": "^5.3.1",
    "image-size": "^1.0.0",
    "imagemin-mozjpeg": "^9.0.0",
    "imagemin-pngquant": "^9.0.2",
    "imagemin-svgo": "^9.0.0",
    "npm-run-all": "^4.1.5",
    "postcss-custom-media": "^8.0.0",
    "postcss-import": "^14.0.2",
    "postcss-loader": "^5.3.0",
    "postcss-nested": "^5.0.5",
    "stylelint": "^13.13.1",
    "stylelint-config-recess-order": "^2.4.0",
    "stylelint-high-performance-animation": "^1.5.2",
    "stylelint-scss": "^3.19.0",
    "stylelint-webpack-plugin": "^2.1.1",
    "through2": "^2.0.5",
    "ts-loader": "^9.1.2",
    "typescript": "^4.2.4",
    "webpack": "^5.37.0",
    "webpack-cli": "^4.7.0",
    "webpack-fix-style-only-entries": "^0.6.1",
    "webpack-merge": "^5.7.3"
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0"
  }
}
