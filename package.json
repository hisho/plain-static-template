{
  "name": "plain-static-template",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=14.8.0"
  },
  "scripts": {
    "preinstall": "npx check-engines && npx only-allow npm",
    "postinstall": "NODE_ENV=production MODE=development run-s gulp-mkdir",
    "prestart": "run-s _pre",
    "start": "NODE_ENV=development run-s _start",
    "start-pro": "NODE_ENV=production MODE=start run-s _start",
    "prebuild": "run-s _pre",
    "build": "NODE_ENV=production MODE=build run-s _build",
    "build-dev": "NODE_ENV=development MODE=build run-s _build",
    "postbuild": "run-s _post",
    "serve": "NODE_ENV=production MODE=build run-s build server",
    "server": "browser-sync start --config ./.config/bs-config.js",
    "lint": "run-s next-lint prettier",
    "next-lint": "next lint --dir src --fix",
    "prettier": "prettier --write './src/**/*.{ts,tsx}'",
    "gulp-clean": "gulp clean",
    "gulp-clone": "gulp clone",
    "gulp-mkdir": "gulp mkdir",
    "gulp-image": "gulp image",
    "gulp-generateImageConstant": "gulp generateImageConstant",
    "gulp-html": "gulp html",
    "gulp-watch": "gulp watch",
    "gulp-sprite": "gulp sprite",
    "gulp-build": "run-p gulp-clone gulp-image gulp-generateImageConstant gulp-sprite",
    "webpack-watch": "webpack -w",
    "webpack-build": "webpack",
    "watch:gulp-webpack": "run-p gulp-watch webpack-watch",
    "build:gulp-webpack": "run-p gulp-build webpack-build",
    "_pre": "run-s gulp-clean",
    "_post": "run-s gulp-html",
    "_start": "run-p server gulp-build watch:gulp-webpack",
    "_build": "run-s build:gulp-webpack"
  },
  "devDependencies": {
    "@types/browser-sync": "^3001.0.28-alpha",
    "@types/eslint": "^7.28.0",
    "@types/fs-extra": "^9.0.12",
    "@types/glob": "^7.1.4",
    "@types/gulp": "^4.0.9",
    "@types/gulp-imagemin": "^7.0.3",
    "@types/gulp-rename": "^2.0.1",
    "@types/gulp-svg-sprite": "^1.2.32",
    "@types/imagemin-mozjpeg": "^8.0.1",
    "@types/imagemin-pngquant": "^8.0.0",
    "@types/imagemin-svgo": "^9.0.1",
    "@types/lodash": "^4.14.171",
    "@types/node": "^16.3.1",
    "@types/postcss-import": "^12.0.1",
    "@types/postcss-nested": "^4.2.3",
    "@types/prettier": "^2.3.2",
    "@types/react": "^17.0.14",
    "@types/react-dom": "^17.0.9",
    "@types/react-helmet": "^6.1.2",
    "@types/stylelint": "^13.13.1",
    "@types/through2": "^2.0.36",
    "@types/webpack": "^5.28.0",
    "autoprefixer": "^10.3.1",
    "browser-sync": "^2.27.4",
    "chalk": "^4.1.1",
    "css-loader": "^5.2.6",
    "dayjs": "^1.10.6",
    "del": "^6.0.0",
    "eslint": "^7.30.0",
    "eslint-config-next": "^11.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-tailwindcss": "^1.14.1",
    "extract-css-chunks-webpack-plugin": "^4.9.0",
    "fork-ts-checker-webpack-plugin": "^6.2.12",
    "fs-extra": "^10.0.0",
    "glob": "^7.1.7",
    "gulp": "^4.0.2",
    "gulp-htmlhint": "^4.0.1",
    "gulp-imagemin": "^7.1.0",
    "gulp-notify": "^4.0.0",
